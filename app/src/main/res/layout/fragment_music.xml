<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/new_background"
    tools:context=".MusicFragment"
    android:id="@+id/frameLayout">


    <View
        android:id="@+id/view6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btnVoltarSeguro"
        android:layout_width="58dp"
        android:layout_height="55dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="353dp"
        android:background="@drawable/seta"
        app:layout_constraintEnd_toEndOf="@+id/view6"
        app:layout_constraintStart_toStartOf="@+id/view6"
        app:layout_constraintTop_toTopOf="@+id/view6" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="116dp"
        android:layout_height="105dp"
        android:layout_marginStart="147dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="148dp"
        android:src="@drawable/ic_seguroo"
        app:layout_constraintEnd_toEndOf="@+id/view6"
        app:layout_constraintStart_toStartOf="@+id/view6"
        app:layout_constraintTop_toTopOf="@+id/view6" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="272dp"
        android:layout_height="426dp"
        android:layout_gravity="center"
        android:layout_marginStart="69dp"
        android:layout_marginTop="134dp"
        android:layout_marginEnd="70dp"
        android:gravity="center"
        android:text="Um App totalmente seguro e confiavel, pode ficar tranquilo os links em que você vai entrar vão te levar para a PlayStore ✅ (Jogos populares jogados por varias pessoas ao seu redor)"
        android:textColor="@color/black"
        android:textSize="31sp"
        app:layout_constraintEnd_toEndOf="@+id/view6"
        app:layout_constraintStart_toStartOf="@+id/view6"
        app:layout_constraintTop_toTopOf="@+id/view6" />


</androidx.constraintlayout.widget.ConstraintLayout>